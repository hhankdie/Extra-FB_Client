_UN8_ = '\033[38;5;86m'
_UN9_ = '\033[38;5;69m'
_UN10_ = '\033[38;5;205m'
NextDay = datetime.datetime.today() + datetime.timedelta(days=1)
NextDay = NextDay.strftime("%Y-%m-%d")

def _ICV2_(_LeThanh_, end, _Text_Delay_, _Text_Tre_):
			for i in range(len(_LeThanh_)):
						sys.stdout.write(_LeThanh_[i])
						sys.stdout.flush()
						sleep(_Text_Delay_)
			print(end=end)
			sleep(_Text_Tre_)

def _CHECK_KEY_():
            _DATE_ = requests.get('https://api.api-ninjas.com/v1/worldtime?city=Nam%20Dinh', headers={'Origin': 'https://api-ninjas.com'}).json()['date']
            _FILE_KEY_ = f'KEY_{_DATE_}.txt'
            _CHECK_EXIST_ = os.path.exists(_FILE_KEY_)

            if _CHECK_EXIST_ == False:
                        _ICV2_(f'[üå∏] {_UN10_}Kh√¥ng Ph√°t Hi·ªán Key Tr∆∞·ªõc ƒê√≥, T·ª± ƒê·ªông Chuy·ªÉn H∆∞·ªõng L·∫•y Key Sau 5s...', end='', _Text_Delay_=0.009, _Text_Tre_=5)
                        print(f'\r                                      {_DEFAULT_}                                        \r', end='')
                        _ICV2_(f'{_GREEN_}Link Ch·ª©a KEY: {_YELLOW_}{_WEB1S_KEY_}{_GREEN_} | ', end='', _Text_Delay_=0.009, _Text_Tre_=5)
                        _CHECK_KEY_ = input(f'Nh·∫≠p KEY C·ªßa B·∫°n: {_YELLOW_}')
                        if _CHECK_KEY_ == _FINAL_KEY_:
                                    sleep(3)
                                    _ICV2_(f'[üå∏] {_UN10_}KEY ƒê√£ ƒê∆∞·ª£c ACTIVE! Vui L√≤ng Restart Client...', end='\n', _Text_Delay_=0.009, _Text_Tre_=5)
                                    with open(f'KEY_{_DATE_}.txt', "w") as file:
                                                file.write(_CHECK_KEY_)
                                    exit()
                        else:
                                    sleep(10)
                                    _ICV2_(f'{_RED_}[üå∏] {_RED_}KEY Sai Ho·∫∑c Ch∆∞a ƒê∆∞·ª£c ACTIVE! Vui L√≤ng Th·ª≠ L·∫°i...', end='', _Text_Delay_=0.009, _Text_Tre_=5)
                                    exit()
            else:
                        _ICV2_(f'[üå∏] {_UN10_}ƒê√£ Ph√°t Hi·ªán Key Tr∆∞·ªõc ƒê√≥, T·ª± ƒê·ªông Chuy·ªÉn H∆∞·ªõng Ki·ªÉm Tra Key Sau 5s...', end='', _Text_Delay_=0.009, _Text_Tre_=5)
                        print(f'\r                                      {_DEFAULT_}                                        \r', end='')
                        with open(f'KEY_{_DATE_}.txt', "r") as file:
                                    _NOI_DUNG_KEY_ = file.read()
                                    if _NOI_DUNG_KEY_ == _FINAL_KEY_:
                                                _ICV2_(f'{_GREEN_}[ KEY: {_YELLOW_}{_NOI_DUNG_KEY_} {_GREEN_}| TYPE: {_YELLOW_}TRIAL 1-DAY {_GREEN_}| ACTIVE: {_YELLOW_}{_DATE_} {_GREEN_}| EXPIRED: {_YELLOW_}{NextDay} {_GREEN_}]{_DEFAULT_}', end='\n', _Text_Delay_=0.009, _Text_Tre_=5)
                                    else:
                                                try:
                                                            os.remove(f'KEY_{_DATE_}.txt')
                                                            _ICV2_(f'[üå∏] {_RED_}Key Sai Ho·∫∑c Kh√¥ng T·ªìn T·∫°i Tr√™n H·ªá Th·ªëng! Key ƒê√£ B·ªã X√≥a...', end='', _Text_Delay_=0.009, _Text_Tre_=5)
                                                            exit()
                                                except:
                                                            _ICV2_(f'[üå∏] {_RED_}Key Sai Ho·∫∑c Kh√¥ng T·ªìn T·∫°i Tr√™n H·ªá Th·ªëng! Vui L√≤ng X√≥a Key Th·ªß C√¥ng...', end='', _Text_Delay_=0.009, _Text_Tre_=5)
                                                            exit()

try:
			_CHECK_KEY_()
except:
			None
