import requests, json, os, sys, datetime
from time import sleep
_RED_ = '\033[38;5;196m'
_DEFAULT_ = '\033[0m'
_GREEN_ = '\033[38;5;40m'
_YELLOW_ = '\033[1;33m'
_BLUE_ = '\033[34m'
_PURPLE_ = '\033[35m'
_BLUE2_ = '\033[36m'
_UNKNOWN_ = '\033[38;5;226m'
_UN1_ = '\033[38;5;129m'
_UN2_ = '\033[38;5;105m'
_UN3_ = '\033[38;5;202m'
_UN4_ = '\033[38;5;168m'
